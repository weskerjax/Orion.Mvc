@model List<TestParamsViewModel>

@{
	ViewBag.Title = "BsRadioList";

	var items = new Dictionary<string, string> {
		{"W", "W"},
		{"B", "B"},
	};

}

@for (int i = 0; i < Model.Count; i++)
{
	<div>
		@Html.BsRadioListFor(m => m[i].InvoicePrefix, items, new { @class = "radio-inline" })
	</div>
}



